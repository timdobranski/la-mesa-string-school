      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "string-school",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.timdobranski.lamesastringschool"
