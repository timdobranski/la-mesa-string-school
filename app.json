      //     }
      //   ]
      // }

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.timdobranski.lamesastringschool"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ]
  }
